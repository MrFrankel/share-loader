{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "shared": {
      "root": "projects/company/shared",
      "sourceRoot": "projects/company/shared/src",
      "projectType": "library",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/company/shared/tsconfig.lib.json",
            "project": "projects/company/shared/ng-package.json"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/company/shared/src/test.ts",
            "tsConfig": "projects/company/shared/tsconfig.spec.json",
            "karmaConfig": "projects/company/shared/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/company/shared/tsconfig.lib.json",
              "projects/company/shared/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**"]
          }
        }
      }
    },
    "ext-app": {
      "root": "projects/ext-app/",
      "sourceRoot": "projects/ext-app/src",
      "projectType": "library",
      "prefix": "ext-app",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@angular-builders/custom-webpack:browser",
          "options": {
            "customWebpackConfig": {
              "path": "projects/ext-app/webpack.config.js",
              "mergeStrategies": {
                "entry": "replace",
                "externals": "replace"
              }
            },
            "outputPath": "dist/ext-app",
            "index": "projects/ext-app/src/index.html",
            "main": "projects/ext-app/src/main.ts",
            "tsConfig": "projects/ext-app/tsconfig.lib.json",
            "assets": [],
            "styles": [],
            "scripts": [],
            "optimization": false,
            "outputHashing": "none",
            "sourceMap": false,
            "extractCss": false,
            "namedChunks": false,
            "aot": true,
            "extractLicenses": false,
            "vendorChunk": false,
            "buildOptimizer": true
          }
        },
        "serve": {
          "builder": "@angular-builders/dev-server:generic",
          "options": {
            "browserTarget": "ext-app:build",
            "port": 4300
          },
          "configurations": {
            "external": {
              "browserTarget": "ext-app:build:external"
            }
          }
        }
      }
    },
    "shell-app": {
      "root": "projects/shell-app/",
      "sourceRoot": "projects/shell-app/src",
      "projectType": "library",
      "prefix": "shell-app",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@angular-builders/custom-webpack:browser",
          "options": {
            "customWebpackConfig": {
              "path": "projects/shell-app/webpack.config.js"
            },
            "outputPath": "dist/shell-app",
            "index": "projects/shell-app/src/index.html",
            "main": "projects/shell-app/src/main.ts",
            "polyfills": "projects/shell-app/src/polyfills.ts",
            "tsConfig": "projects/shell-app/tsconfig.app.json",
            "assets": [
              "projects/shell-app/src/favicon.ico",
              "projects/shell-app/src/assets"
            ],
            "styles": ["projects/shell-app/src/styles.css"],
            "scripts": [],
            "es5BrowserSupport": true,
            "optimization": false,
            "outputHashing": "all",
            "sourceMap": false,
            "extractCss": true,
            "namedChunks": false,
            "aot": true,
            "extractLicenses": true,
            "vendorChunk": false,
            "buildOptimizer": true
          }
        },
        "serve": {
          "builder": "@angular-builders/dev-server:generic",
          "options": {
            "browserTarget": "shell-app:build",
            "port": 4200
          }
        }
      }
    }
  },
  "defaultProject": "shell-app"
}
